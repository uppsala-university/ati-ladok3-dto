          <xs:element minOccurs="0" name="Text" type="xs:string"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="Datumperiod">
<xs:annotation>
<xs:documentation><![CDATA[Enkel representation som innehåller två datum - från och till.]]></xs:documentation>
</xs:annotation>
