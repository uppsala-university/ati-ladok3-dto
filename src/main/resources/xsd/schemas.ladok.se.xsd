<xs:annotation>
<xs:documentation><![CDATA[Lista av grunddatatyper]]></xs:documentation>
</xs:annotation>
    <xs:sequence>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="Grunddatakategorier" type="base:Grunddatatyp"/>
