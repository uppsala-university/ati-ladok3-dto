          <xs:element minOccurs="0" name="Efternamn" type="xs:string"/>
          <xs:element minOccurs="0" name="Fornamn" type="xs:string"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType abstract="true" name="BaseEntitet">
<xs:annotation>
<xs:documentation><![CDATA[(param): <T> Typ som ärver {@link BaseEntitetRepresentation}]]></xs:documentation>
