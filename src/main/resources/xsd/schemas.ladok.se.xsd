  <xs:complexType name="LadokException">
    <xs:complexContent>
      <xs:extension base="base:BaseEntitet">
        <xs:sequence>
          <xs:element minOccurs="0" name="Detaljkod" type="xs:string"/>
          <xs:element minOccurs="0" name="DetaljkodText" type="xs:string"/>
          <xs:element minOccurs="0" name="FelUID" type="xs:string"/>
          <xs:element minOccurs="0" name="Felgrupp" type="xs:string"/>
          <xs:element minOccurs="0" name="FelgruppText" type="xs:string"/>
          <xs:element minOccurs="0" name="Felkategori" type="xs:string"/>
          <xs:element minOccurs="0" name="FelkategoriText" type="xs:string"/>
          <xs:element minOccurs="0" name="FelkallaAttributID" type="xs:string">
<xs:annotation>
<xs:documentation><![CDATA[Identifierar det attribut som har orsakat felet. Detta kan vara bra att ha om man vill veta exakt vilket attribut på ett objekt
som orsakat felet.]]></xs:documentation>
</xs:annotation>
</xs:element>
          <xs:element minOccurs="0" name="FelkallaObjektID" type="xs:string">
<xs:annotation>
<xs:documentation><![CDATA[Identifierar det objekt som har orsakat felet. Detta kan vara bra att ha om man har många objekt som valideras samtidigt, t.ex.
en lista av entiteter och man som anropare vill veta vilket av objekten i listan som orsakat felet.]]></xs:documentation>
</xs:annotation>
</xs:element>
          <xs:element minOccurs="0" name="Meddelande" type="xs:string"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="Links">
<xs:annotation>
<xs:documentation><![CDATA[Generell representation för länkar med rel's]]></xs:documentation>
</xs:annotation>
    <xs:complexContent>
      <xs:extension base="dap:Base">
        <xs:sequence/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

